{
  "name": "@netatwork/spy",
  "version": "0.0.2-0",
  "description": "Proxy-based spy implementation",
  "main": "dist/Spy.js",
  "typings": "dist/types/Spy",
  "scripts": {
    "prebuild": "rimraf dist & eslint . --ext .ts --cache",
    "lint": "eslint . --ext .ts --cache",
    "build": "tsc",
    "dev": "tsc -w --preserveWatchOutput",
    "gen:changelog": "conventional-changelog -i CHANGELOG.md -s -r 2"
  },
  "keywords": [
    "test",
    "spy"
  ],
  "author": "Net at Work GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netatwork-de/spy.git"
  },
  "bugs": {
    "url": "https://github.com/Netatwork-de/spy/issues"
  },
  "homepage": "https://github.com/Netatwork-de/spy#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/chai": "^4.2.18",
    "@types/node": "^15.9.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.1.2",
    "husky": "^6.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "typescript": "^4.3.4"
  },
  "optionalDependencies": {
    "chai": "4.x"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
